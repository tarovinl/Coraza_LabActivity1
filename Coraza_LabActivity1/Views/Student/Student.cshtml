@model Student;

<h1>Student Details</h1>

<table class="table table-striped table-dark">
    <tr>
        <th>Id</th>
        <td>@Model.Id</td>
    </tr>
    <tr>
        <th>Name</th>
        <td>@Model.Name</td>
    </tr>
    <tr>
        <th>Course</th>
        <td>@Model.Course</td>
    </tr>
    <tr>
        <th>Date Enrolled</th>
        <td>@Model.DateEnrolled</td>
    </tr>
</table>
